{
  "name": "site",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev",
    "build": "astro check && astro build --remote",
    "preview": "astro preview",
    "astro": "astro",
    "deploy": "npm run build && npm run deploy-upload-client && npm run deploy-upload-server && npm run deploy-restart-server",
    "deploy-upload-client": "rsync -lprv --delete dist/client/ nonacademic.net:/srv/latex/",
    "deploy-upload-server": "rsync -lprv --delete --relative dist/server/ package.json package-lock.json nonacademic.net:latex-server/",
    "deploy-restart-server": "ssh nonacademic.net ./latex-server-restart"
  },
  "dependencies": {
    "@astrojs/check": "^0.9.4",
    "@astrojs/db": "^0.14.5",
    "@astrojs/mdx": "^4.0.6",
    "@astrojs/node": "^9.0.1",
    "@astrojs/react": "^4.1.2",
    "@astrojs/rss": "^4.0.11",
    "@astrojs/sitemap": "^3.2.1",
    "@astrojs/svelte": "^7.0.3",
    "@astrojs/vue": "^5.0.4",
    "@types/d3": "^7.4.3",
    "@types/react": "^19.0.4",
    "@types/react-dom": "^19.0.2",
    "astro": "^5.1.3",
    "d3": "^7.9.0",
    "marked": "^15.0.6",
    "marked-base-url": "^1.1.6",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "svelte": "^5.17.3",
    "typescript": "^5.7.3",
    "vue": "^3.5.13"
  },
  "devDependencies": {
    "@types/node": "^22.10.7"
  }
}
